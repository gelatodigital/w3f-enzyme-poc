{
  "name": "gelato-nft-web3-function",
  "version": "0.0.1",
  "description": "Create Hello World tasks using Gelato Ops SDK",
  "scripts": {
    "format": "prettier --write '*/**/*.*{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.*{js,json,md,ts}'",
    "lint": "eslint -c .eslintrc.json --ext \"**/*.ts\"",
    "deploy": "npx hardhat run scripts/deploy-contract.ts",
    "w3f:deploy": "npx w3f deploy web3-functions/enzyme/index.ts",
    "test": "npx w3f test web3-functions/enzyme/exitLP.ts  --show-logs"
  },
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.6",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@openzeppelin/contracts": "^4.8.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/chance": "^1.1.3",
    "@types/glob": "^8.1.0",
    "@types/node": "16.7.10",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.7",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-deploy": "^0.11.25",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  },
  "lint-staged": {
    "*.*{js,json,md,ts,yml,yaml}": "prettier --write",
    "*.*{ts,js}": "eslint -c .eslintrc.json"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@gelatonetwork/ops-sdk": "^2.2.0-alpha",
    "@gelatonetwork/web3-functions-sdk": "^0.4.3",
    "@paraswap/sdk": "^6.1.2",
    "@uniswap/sdk-core": "^3.1.0",
    "@uniswap/v3-sdk": "^3.9.0",
    "axios": "^1.3.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "graphql-tag": "^2.12.6",
    "ky": "^0.33.2"
  }
}
